msgid " Photo Magenta [6]\n"
msgstr "  フォト マゼンタ  [6]\n"

#: src/bjcupsmon_ui.c:49 src/bjcupsmon_ui.c:70
msgid " Cyan [6]\n"
msgstr "  シアン  [6]\n"

#: src/bjcupsmon_ui.c:50 src/bjcupsmon_ui.c:71
msgid " Magenta [6]\n"
msgstr "  マゼンタ  [6]\n"

#: src/bjcupsmon_ui.c:51 src/bjcupsmon_ui.c:72
msgid " Yellow [6]\n"
msgstr "  イエロー  [6]\n"

#: src/bjcupsmon_ui.c:52 src/bjcupsmon_ui.c:73
msgid " Red [6]\n"
msgstr "  レッド  [6]\n"

