#: po/keystr.h:230
msgid "No"
msgstr "No"

#: po/keystr.h:231
msgid "Abort"
msgstr " "

#: po/keystr.h:233
msgid "Enable"
msgstr "Abilita"

#: po/keystr.h:234
msgid "Disable"
msgstr "Disattivato"

#: po/keystr.h:235
msgid "5 minutes"
msgstr "5 minuti"

#: po/keystr.h:236
msgid "10 minutes"
msgstr "10 minuti"

#: po/keystr.h:237
msgid "15 minutes"
msgstr "15 minuti"

#: po/keystr.h:238
msgid "30 minutes"
msgstr "30 minuti"

#: po/keystr.h:239
