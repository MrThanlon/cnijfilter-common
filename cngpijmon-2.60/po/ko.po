#: src/bjcupsmon_ui.c:83
msgid " Yellow [7]\n"
msgstr " 옐로 [7]\n"

#: src/bjcupsmon_ui.c:84 src/bjcupsmon_ui.c:112
msgid " Red [7]\n"
msgstr " 빨간색 [7]\n"

#: src/bjcupsmon_ui.c:85 src/bjcupsmon_ui.c:113
msgid " Green [7]\n"
msgstr " 초록색 [7]\n"

